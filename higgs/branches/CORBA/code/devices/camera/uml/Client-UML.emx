image format conversion functions

static inline
void iyu12yuy2 (char *src, char *dest, int NumPixels) {
int i=0,j=0;
register int y0, y1, y2, y3, u, v;
while (i &lt; NumPixels*3/2) {
u = src[i++];
y0 = src[i++];
y1 = src[i++];
v = src[i++];
y2 = src[i++];
y3 = src[i++];

dest[j++] = y0;
dest[j++] = u;
dest[j++] = y1;
dest[j++] = v;

dest[j++] = y2;
dest[j++] = u;
dest[j++] = y3;
dest[j++] = v;
}
}
static inline
void rgb2yuy2 (char *RGB,char *YUV, int NumPixels) {
int i, j;
register int y0, y1, u0, u1, v0, v1 ;
register int r, g, b;

for (i = 0, j = 0; i &lt; 3 * NumPixels; i += 6, j += 4) {
r = RGB[i + 0];
g = RGB[i + 1];
b = RGB[i + 2];
RGB2YUV (r, g, b, y0, u0 , v0);
r = RGB[i + 3];
g = RGB[i + 4];
b = RGB[i + 5];
RGB2YUV (r, g, b, y1, u1 , v1);
YUV[j + 0] = y0;
YUV[j + 1] = (u0+u1)/2;
YUV[j + 2] = y1;
YUV[j + 3] = (v0+v1)/2;
}
}
